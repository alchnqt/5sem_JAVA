<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">
    <class>com.example.lab1.model.ApplicationUser</class>
    <class>com.example.lab1.model.Role</class>
    <class>com.example.lab1.model.Song</class>
    <properties>
      <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:5432/spring"/>
      <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
